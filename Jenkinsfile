pipeline {
  agent any

  stages {
    stage ('inicial') {
      steps {
        echo 'Ola eu sou o FV, vou te servir nesse momento.'
        echo 'Irei iniciar o processo da pipeline, isso pode demorar alguns minutos, te encontro no fim...'
      }
    }
    stage ('Build Image') {
      steps {
        echo 'Iniciando build da imagem.'
      }
    }
  }

}
